{
  "name": "hooperui",
  "version": "0.0.1",
  "description": "A new HooperUI UIkit",
  "main": "index.js",
  "typings": "typings/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "postinstall": "cd website && npm install",
    "test": "npx jest --config tests/unit/jest.config.js",
    "test:coverage": "npx jest --config tests/unit/jest.config.js --coverage",
    "test:unit": "npx jest --config tests/unit/jest.config.js --watch",
    "doc:build": "cd docs && mkdocs build --clean",
    "doc:dev": "cd docs && mkdocs serve",
    "dev": "node build/dev",
    "dev:demo": "npx webpack-dev-server --config ./build/webpack.dev.js --open-page 'dist/'",
    "build": "node build/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hooper93/HooperUI.git"
  },
